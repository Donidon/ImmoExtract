{
    "name": "Immo extractor",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Extracteur de donn√©es de SeLoger et Leboncoin",    
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_locale": "fr",
    "background": {
      "page": "src/bg/background.html",
      "persistent": false
    },  
    "page_action": {
      "default_icon": "icons/icon19.png",
      "default_title": "Extractor Title",
      "default_popup": "src/page_action/page_action.html"
    },
    "permissions": [
      "https://www.seloger.com/*",
      "https://www.leboncoin.fr/*",
      "storage",
      "declarativeContent",
      "activeTab",
      "tabs",
      "clipboardWrite"
    ]
  } 